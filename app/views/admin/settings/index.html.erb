<%= form_for(@preference, url: admin_settings_path(@preference.id), method: "patch") do |f| %>
  <%= f.radio_button :song_sort_order, "DESC" %>
  <%= f.radio_button :song_sort_order, "ASC" %><br><br>

  <%= f.radio_button :artist_sort_order, "DESC" %>
  <%= f.radio_button :artist_sort_order, "ASC" %>

  <%= f.submit "Update Preferences" %>


<% end %>
